<%

    getZombieImgUrl = function(zombieName){
        zombieName = zombieName.toUpperCase().replace(/\s/g, '');
        var imgLocation = "./images/unit_icon/zombie_normal.png";

        if(zombieName == "Balloon Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_balloon.png";
        }else if(zombieName == "BucketHead Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_buckethead.png";
        }else if(zombieName == "BackupDancer Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_dancer.png";
        }else if(zombieName == "Dolphin Rider Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_dolphinrider.png";
        }else if(zombieName == "Ducky Tube Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_duckytube.png";
        }else if(zombieName == "Gargantuar Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_gargantuar.png"
        }else if(zombieName == "Ladder Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_ladder.png";
        }else if(zombieName == "Snorkel Zombie".toUpperCase().replace(/\s/g, '')) {
            imgLocation = "./images/unit_icon/zombie_snorkel.png";
        }

        return imgLocation;
    };    

%>