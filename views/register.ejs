<% include ./partials/accountheader %>

	<!--form start-->
	<form class="form-container animated fadeIn" action="/register" method="POST">
	  	<img src="/images/logo.png" width="320" height="60" alt=""><br><br>
		<h2>Register</h2>
		<div class="form-group">
	      	<label for="inputName">Name</label>
	      	<input type="text" class="form-control" id="inputName" name="name">
	    </div>
	    <div class="form-group">
	      	<label for="inputUsername">Username</label>
	      	<input type="text" class="form-control" id="inputUsername" name="username">
	    </div>
	    <div class="form-group">
	      	<label for="inputPassword">Password</label>
	      	<input type="password" class="form-control" id="inputPassword" name="password">
		</div><br>
	    <button type="submit" onClick="showLoader()" class="btn btn-success btn-block">Register</button>
	</form>
	<!--form end-->

<% include ./partials/accountfooter %>